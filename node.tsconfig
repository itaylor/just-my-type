{
  "include": [ "*.ts"],
  "exclude": [ "*.test.ts" ],
  "compilerOptions": {
    "declaration": true,
    "outDir": "./node",
    "allowJs": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "inlineSourceMap": true,
    "isolatedModules": true,
    "module": "commonjs",
    "strict": true,
    "target": "es2019",
    "useDefineForClassFields": true
  }
}